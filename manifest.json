{
  "manifest_version": 3,
  "name": "GPT Translator",
  "version": "1.0",
  "description": "Translate with ChatGPT",
  "permissions": ["activeTab", "storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.js"]
  }]
}